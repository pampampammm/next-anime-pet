{
  "trailingComma": "es5",
  "tabWidth": 4,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "importOrder": [
    "^@/app/widgets/(.*)$",
    "^@/app/pages/(.*)$",
    "^@/app/feature/(.*)$",
    "^@/app/entity/(.*)$",
    "^@/app/shared/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "(.scss)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
}